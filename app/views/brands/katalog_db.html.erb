      <div style="margin-left:300px;">
      <% for c in @categories %>
        <p><b><%= c.id -%> - <%= c.title -%> </b></p>
           <% for c2 in c.children %>
             <li><u><%= c2.id -%> - <%= c2.title -%></u>
             (
              <%  bs = []
                  c2.batches.each do |b| bs << b.brand.title end
               %>
               <%= bs.uniq.join(', ') %>
             )
             <ul style="margin-left:50px;">
             <% for b in c2.batches %>
               <li><%= b.id -%> - <%= b.brand.title -%> <%= b.title -%></li>
             <% end %>
             </ul>

             <ul style="margin-left:20px;">
             <% for c3 in c2.children %>
               <li><%= c3.id -%> - <%= c3.title -%>
             (
              <%  bs = []
                  c3.batches.each do |b| bs << b.brand.title end
               %>
